global proc AENBuddyParticleEmitterTemplate ( string $nodeName )
{
editorTemplate -beginScrollLayout;
	AEtransformMain $nodeName;
	editorTemplate -beginLayout "EmitterSettings"  -collapse 0;
		editorTemplate -callCustom "NBuddy_BodyPopUpMenuNew" "NBuddy_BodyPopUpMenuReplace" "inBody";
	editorTemplate -endLayout;	
	editorTemplate -addExtraControls;
editorTemplate -endScrollLayout;
}
